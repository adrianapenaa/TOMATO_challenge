    "# function for printing out and testing sentences\n",
    "\n",
    "def print_sentence(df, sentence_id=None):\n",
    "    if not sentence_id:\n",
    "        print(\"No ID given\")\n",
    "    print(\"Sentence ID = {}\".format(sentence_id))\n",
    "    return df[df.SentenceId == sentence_id].iloc[:].Phrase\n",
    "\n",
